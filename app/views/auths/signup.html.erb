<div class='container'>
  <h2>Signup</h2>
  <%= display_error %>
  <%= form_for :auth do |f| %>
    Username<br>
    <%= f.text_field :user_name %><br>
    Password<br>
    <%= f.password_field :password %><br>
    <%= f.password_field :password_confirmation %><br>
    <%= f.submit 'Signup' %>
  <% end %>
</div>
